  "devDependencies": {
    "cypress": "^7.2.0",
    "cypress-xpath": "^1.6.2",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "dependencies": {
    "typescript": "^4.2.4"
  }
}
